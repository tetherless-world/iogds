<http://logd.tw.rpi.edu/source/env-gov-bc-ca/dataset/catalog/version/2011-Jul-13/conversion/enhancement/1>
   a conversion:LayerDataset, void:Dataset;

   conversion:base_uri           "http://logd.tw.rpi.edu"^^xsd:anyURI;
   conversion:source_identifier  "env-gov-bc-ca";
   conversion:dataset_identifier "catalog";
   conversion:version_identifier "2011-Jul-13";

   a conversion:DatasetCatalog, conversion:MetaDataset;   #dataset catalog addition
   dgtwc:catalog_title "Env.gov.bc.ca (British Columbia, Canada)" ;
   dgtwc:catalog_homepage <http://www.env.gov.bc.ca/cas/data/> ;
   dgtwc:catalog_country  dbpedia:Canada ;


   conversion:conversion_process [
      a conversion:EnhancementConversionProcess;
      conversion:enhancement_identifier "1";
      conversion:subject_discriminator  "env.gov.bc.ca";

      dcterms:creator <http://tw.rpi.edu/web/inside/machine/sam#sym>;
      dcterms:created "2011-07-14T11:45:07-04:00"^^xsd:dateTime;

      conversion:delimits_cell ",";
      #conversion:delimits_cell "	"; # tab
      #conversion:delimits_cell "|";   # pipe
      #conversion:delimits_cell ",";   # comma

      #conversion:interpret [          
      #   conversion:symbol        "";
      #   conversion:interpretation conversion:null; 
      #];                            
      
      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate rdf:type;
         conversion:object    conversion:CatalogedDataset; 
      ];
      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate dgtwc:catalog_subtitle;
         conversion:object    ""; 
      ];
      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate dgtwc:catalog_title;
         conversion:object    "Env.gov.bc.ca (British Columbia, Canada)" ;
      ];
      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate dgtwc:catalog_homepage;
         conversion:object    <http://www.env.gov.bc.ca/cas/data/> ;
      ];
      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate dgtwc:catalog_country;
         conversion:object    dbpedia:Canada ; 
      ];

      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate dcterms:description;
         conversion:object    "n/a"; 
      ];

      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate dcterms:subject;
         conversion:object    "n/a"; 
      ];
      
      
      
      conversion:enhance [
         ov:csvCol          1;
         ov:csvHeader       "Category";
         #conversion:label   "Category";
         conversion:comment "";
         conversion:range   todo:Literal;
         
         conversion:subproperty_of dgtwc:category ;	#dataset catalog addition
         conversion:subproperty_of dgtwc:categories;
         conversion:interpret [
            conversion:symbol "";
            conversion:interpretation "n/a";
         ];
         conversion:interpret [
            conversion:symbol "(none)";
            conversion:interpretation "n/a";
         ];                  
         
      ];
      conversion:enhance [
         ov:csvCol          2;
         ov:csvHeader       "Sub-topic";
         #conversion:label   "Sub-topic";
         conversion:comment "";
         conversion:range   todo:Literal;
      ];
      conversion:enhance [
         ov:csvCol          3;
         ov:csvHeader       "Title";
         #conversion:label   "Title";
         conversion:comment "";
         conversion:range   todo:Literal;
         
         conversion:subproperty_of dcterms:title;	#dataset catalog addition
         conversion:interpret [
            conversion:symbol "";
            conversion:interpretation "n/a";
         ];
         
      ];
      conversion:enhance [
         ov:csvCol          4;
         ov:csvHeader       "Link";
         #conversion:label   "Link";
         conversion:comment "";
                  
         conversion:range  rdfs:Resource;		
         conversion:subproperty_of foaf:homepage;	#dataset catalog addition
         conversion:interpret [
            conversion:symbol "";
            conversion:interpretation conversion:null;
         ];
         
      ];
      conversion:enhance [
         ov:csvCol          5;
         ov:csvHeader       "Agency";
         #conversion:label   "Agency";
         conversion:comment "";
                 
         conversion:range  rdfs:Resource;
         conversion:subproperty_of dgtwc:agency ;	#dataset catalog addition
         conversion:subproperty_of dcterms:contributor;
         conversion:interpret [
            conversion:symbol "";
            conversion:interpretation conversion:null;
         ];
         
      ];
      conversion:enhance [
         ov:csvCol          6;
         ov:csvHeader       "Format";
         #conversion:label   "Format";
         conversion:comment "";
         conversion:range   todo:Literal;
      ];
      #conversion:enhance [            
      #   ov:csvRow 3,4,5;             
      #   a conversion:ExampleResource;
      #];                              
   ];
.