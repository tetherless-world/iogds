<http://logd.tw.rpi.edu/source/data-gov-bc-ca/dataset/catalog/version/2011-Sep-15/conversion/enhancement/1>
   a conversion:LayerDataset, void:Dataset;

   conversion:base_uri           "http://logd.tw.rpi.edu"^^xsd:anyURI;
   conversion:source_identifier  "data-gov-bc-ca";
   conversion:dataset_identifier "catalog";
   conversion:version_identifier "2011-Sep-15";

   a conversion:DatasetCatalog, conversion:MetaDataset;   #dataset catalog addition
   dgtwc:catalog_title "Data.gov.bc.ca (British Columbia, Canada)" ;
   dgtwc:catalog_homepage <http://www.data.gov.bc.ca/> ;
   dgtwc:catalog_country dbpedia:Canada;

   conversion:conversion_process [
      a conversion:EnhancementConversionProcess;
      conversion:enhancement_identifier "1";

      dcterms:creator <http://tw.rpi.edu/web/inside/machine/gemini#sym>;
      dcterms:created "2011-09-15T18:47:27-04:00"^^xsd:dateTime;

      conversion:delimits_cell ",";
      #conversion:delimits_cell "	"; # tab
      #conversion:delimits_cell "|";   # pipe
      #conversion:delimits_cell ",";   # comma

      #conversion:interpret [
      #   conversion:symbol        "";
      #   conversion:interpretation conversion:null; 
      #];
      #conversion:enhance [
      #   conversion:domain_template "tool_[r]";
      #   conversion:domain_name     "Tool";
      #];
      #conversion:enhance [
      #   conversion:class_name "Tool";
      #   conversion:subclass_of <http://purl.org/...>;
      #];

      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate rdf:type;
         conversion:object    conversion:CatalogedDataset; 
      ];
      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate dgtwc:catalog_subtitle;
         conversion:object    ""; 
      ];
      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate dgtwc:catalog_title;
         conversion:object    "Data.gov.bc.ca (British Columbia, Canada)" ; 
      ];
      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate dgtwc:catalog_homepage;
         conversion:object    <http://www.data.gov.bc.ca/> ; 
      ];
      conversion:enhance [
         a conversion:SubjectAnnotation; # NOTE: This is NOT a column; it is the row. see https://github.com/timrdf/csv2rdf4lod-automation/issues#issue/82
         conversion:predicate dgtwc:catalog_country;
         conversion:object    dbpedia:Canada ; 
      ];

      conversion:enhance [
         ov:csvCol          1;
         ov:csvHeader       "url";
         #conversion:label   "url";
         conversion:comment "";

         conversion:range  rdfs:Resource;		
         conversion:subproperty_of foaf:homepage;	#dataset catalog addition
         conversion:interpret [          
            conversion:symbol        "";
            conversion:interpretation conversion:null; 
         ];                                     

      ];
      conversion:enhance [
         ov:csvCol          2;
         ov:csvHeader       "Title";
         #conversion:label   "Title";
         conversion:comment "";
         conversion:range   todo:Literal;

         conversion:subproperty_of dcterms:title;	#dataset catalog addition
         conversion:interpret [
            conversion:symbol "";
            conversion:interpretation "n/a";
         ];

      ];
      conversion:enhance [
         ov:csvCol          3;
         ov:csvHeader       "Description";
         #conversion:label   "Description";
         conversion:comment "";
         conversion:range   todo:Literal;

         conversion:subproperty_of dcterms:description;	#dataset catalog addition
         conversion:interpret [
            conversion:symbol "";
            conversion:interpretation "n/a";
         ];

      ];
      conversion:enhance [
         ov:csvCol          4;
         ov:csvHeader       "FileFormat_1";
         #conversion:label   "FileFormat_1";
         conversion:comment "";
         conversion:range   todo:Literal;
      ];
      conversion:enhance [
         ov:csvCol          5;
         ov:csvHeader       "FileURL_1";
         #conversion:label   "FileURL_1";
         conversion:comment "";
         conversion:range   todo:Literal;
      ];
      conversion:enhance [
         ov:csvCol          6;
         ov:csvHeader       "Citation";
         #conversion:label   "Citation";
         conversion:comment "";
         conversion:range   todo:Literal;
      ];
      conversion:enhance [
         ov:csvCol          7;
         ov:csvHeader       "Date Added to Catalogue";
         #conversion:label   "Date Added to Catalogue";
         conversion:comment "";
         conversion:range   todo:Literal;
      ];
      conversion:enhance [
         ov:csvCol          8;
         ov:csvHeader       "Update Frequency";
         #conversion:label   "Update Frequency";
         conversion:comment "";
         conversion:range   todo:Literal;
      ];
      conversion:enhance [
         ov:csvCol          9;
         ov:csvHeader       "Subject";
         #conversion:label   "Subject";
         conversion:comment "";
         conversion:range   todo:Literal;

         conversion:subproperty_of dgtwc:category ;	#dataset catalog addition
         conversion:subproperty_of dgtwc:categories;
         conversion:interpret [
            conversion:symbol "";
            conversion:interpretation "n/a";
         ];
         conversion:interpret [
            conversion:symbol "(none)";
            conversion:interpretation "n/a";
         ];                  

      ];
      conversion:enhance [
         ov:csvCol          10;
         ov:csvHeader       "Sector";
         #conversion:label   "Sector";
         conversion:comment "";
         conversion:range   todo:Literal;
      ];
      conversion:enhance [
         ov:csvCol          11;
         ov:csvHeader       "Ministry";
         #conversion:label   "Ministry";
         conversion:comment "";

         conversion:range  rdfs:Resource;
         conversion:subproperty_of dgtwc:agency ;	#dataset catalog addition
         conversion:subproperty_of dcterms:contributor;
         conversion:interpret [          
            conversion:symbol        "";
            conversion:interpretation conversion:null; 
         ];                            
         conversion:interpret [
            conversion:symbol "(none)";
            conversion:interpretation conversion:null; 
         ];         

      ];
      conversion:enhance [
         ov:csvCol          12;
         ov:csvHeader       "Keywords";
         #conversion:label   "Keywords";
         conversion:comment "";
         conversion:range   todo:Literal;

         conversion:subproperty_of dgtwc:keywords;

         conversion:interpret [
            conversion:symbol "";
            conversion:interpretation "n/a";
         ];
         conversion:object_search [
            conversion:regex     "\\s*([^,]+)";
            conversion:predicate dcterms:subject;
            conversion:object    "[>\\1<]";
         ]; # https://github.com/timrdf/csv2rdf4lod-automation/wiki/conversion:object_search
         
      ];
      conversion:enhance [
         ov:csvCol          13;
         ov:csvHeader       "FileFormat_2";
         #conversion:label   "FileFormat_2";
         conversion:comment "";
         conversion:range   todo:Literal;
      ];
      conversion:enhance [
         ov:csvCol          14;
         ov:csvHeader       "FileURL_2";
         #conversion:label   "FileURL_2";
         conversion:comment "";
         conversion:range   todo:Literal;
      ];
      #conversion:enhance [            
      #   ov:csvRow 3,4,5;             
      #   a conversion:ExampleResource;
      #];                              
   ];
.