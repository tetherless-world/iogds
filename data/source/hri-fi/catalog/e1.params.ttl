   conversion:source_identifier  "hri-fi";
   conversion:dataset_identifier "catalog";
   conversion:version_identifier "2012-Jun-18";
   conversion:enhancement_identifier "1";


   a conversion:DatasetCatalog, conversion:MetaDataset;   #dataset catalog addition
   dgtwc:catalog_title "Hri.fi (Helsinki, Finland)" ;
   dgtwc:catalog_homepage <http://www.hri.fi> ;
   dgtwc:catalog_country  dbpedia:Finland ;
   dgtwc:catalog_language       "Finnish";


   conversion:conversion_process [
      a conversion:EnhancementConversionProcess;
      conversion:enhancement_identifier "1";

      dcterms:creator <http://tw.rpi.edu/web/inside/machine/gemini#sym>;
      dcterms:created "2012-06-18T16:43:28-04:00"^^xsd:dateTime;

      conversion:delimits_cell ",";
      #conversion:delimits_cell "	"; # tab
      #conversion:delimits_cell "|";   # pipe
      #conversion:delimits_cell ",";   # comma

      #conversion:interpret [
      #   conversion:symbol        "";
